# ~/.ratpoisonrc
definekey top C-M-l exec ~/bin/xlock.sh
definekey top C-M-x exec xterm
definekey top C-M-u exec ~/bin/keyboardmap.sh se
definekey top C-M-i exec ~/bin/keyboardmap.sh us

definekey root C-i focus
definekey root g gnext
definekey root G gselect

exec redshift -l 59:18
hsplit
exec xterm
focus
exec xscreensaver -no-splash
#exec gpg-agent emacs
exec emacs
